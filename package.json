{
  "name": "name-that-song-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./src/node/app.js | bunyan",
    "start:dev": "nodemon ./src/node/app.js | bunyan",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:node": "eslint --fix 'src/node/**/*.{js,json}'",
    "prettier:node": "prettier --write 'src/node/**/*.{js,json}'",
    "prettier:angular": "prettier --write 'src/app/**/*.{ts,json,md,html}'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "csurf": "^1.9.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "helmet": "^3.15.0",
    "require-dir": "^1.2.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/node/**/*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/app/**/*.ts}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/app/**/*.{ts,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
