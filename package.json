{
  "name": "angular-name-that-song",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./src/node/app.js | bunyan",
    "start:dev": "nodemon ./src/node/app.js | bunyan",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:node": "eslint --fix 'src/node/**/*.{js,json}'",
    "prettier:node": "prettier --write 'src/node/**/*.{js,json}'",
    "prettier:angular": "prettier --write 'src/app/**/*.{ts,json,md,html}'",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.1",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.1",
    "@angular/compiler": "^8.0.1",
    "@angular/compiler-cli": "^8.0.1",
    "@angular/core": "^8.0.1",
    "@angular/forms": "^8.0.1",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.1",
    "@angular/platform-browser-dynamic": "^8.0.1",
    "@angular/router": "^8.0.1",
    "@mdi/angular-material": "^3.6.95",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "class-transformer": "^0.2.3",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.9",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "hammerjs": "^2.0.8",
    "helmet": "^3.18.0",
    "jsonpath": "^1.0.2",
    "node-cache": "^4.2.0",
    "request": "^2.88.0",
    "require-dir-all": "^0.4.15",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.3",
    "@angular/language-service": "^8.0.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/node/**/*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/app/**/*.ts}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/app/**/*.{ts,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
